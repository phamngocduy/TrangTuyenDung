@model TrangTuyenDung.Models.Apply_Recruitments
<!DOCTYPE html>
<html>
<head>
    <title>@Model.CV_Info.Student_Info.Student_Name</title>

    <meta name="viewport" content="width=device-width" />
    <meta name="description" content="The Curriculum Vitae of Joe Bloggs." />
    <meta charset="UTF-8">

    <link type="text/css" rel="stylesheet" href="~/Content/CSS_Cv/csscuatrangcvtemplate1.css">
    <link href='http://fonts.googleapis.com/css?family=Rokkitt:400,700|Lato:400,300' rel='stylesheet' type='text/css'>
    <!-- CSS Index-->
    <link href="~/Content/csscuatrangindex.css" rel="stylesheet" />

    <!--[if lt IE 9]>
    <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
</head>
<body id="top">
    <div id="cv" class="instaFade">
        <div class="mainDetails">
            <div id="headshot" class="quickFade">
                @*<img src="~/Assets/img/headshot.jpg" alt="" />*@
                <img src="@Url.Content("~/App_Data/Student/Logos/" + Model.CV_Info.Student_Info.Account_Id )" alt="Ảnh đại diện" onError="this.src='@Url.Content("~/App_Data/Student/Logos/Anonymous_User")';">
            </div>

            <div id="name">
                <h1 class="quickFade delayTwo">@Model.CV_Info.Student_Info.Student_Name</h1>
                <hr />
                <span><i class="fas fa-briefcase"></i>  @Model.CV_Info.Student_Info.Faculty.Name</span>
            </div>

            <div id="contactDetails" class="quickFade delayFour">
                <ul>
                    <li>e: <a href="mailto: @Model.CV_Info.Student_Info.AspNetUser.Email" target="_blank">@Model.CV_Info.Student_Info.AspNetUser.Email</a></li>
                    <li>dob: <a href="#"> @Model.CV_Info.Student_Info.Student_Birthday.Value.ToString("dd/MM/yyyy")</a></li>
                    <li>p: @Model.CV_Info.Student_Info.Student_PhoneNumber</li>
                </ul>
            </div>
            <div class="clear"></div>
        </div>

        <div id="mainArea" class="quickFade delayFive">
            <section>
                <article>
                    <div class="sectionTitle">
                        <h1>Giới thiệu bản thân</h1>
                    </div>

                    <div class="sectionContent">
                        @Html.Raw(Model.CV_Info.About_Student)
                    </div>
                </article>
                <div class="clear"></div>
            </section>


            <section>
                <div class="sectionTitle">
                    <h1>Dự án tiêu biểu</h1>
                </div>

                <div class="sectionContent">
                    <article>
                        <h2>Dự án 1</h2>
                        <p style="text-align:left" class="subDetails">2018 - 2019</p>

                        <p>Mô tả dự án 1</p>

                    </article>
                    @*@if (Model.CV_Info.Project_Name_2 != null)
                    {
                        <article>
                            <h2>@Model.CV_Info.Project_Name_2</h2>
                            <p style="text-align:left" class="subDetails">@Model.CV_Info.Project_Date_Start_2 - @Model.CV_Info.Project_Date_End_2</p>
                            @Html.Raw(Model.CV_Info.Project_Description_2)
                        </article>
                    }*@

                    @*<article>
                            <h2>Job Title at Company</h2>
                            <p style="text-align:left" class="subDetails">October 2004 - December 2006</p>
                            <p style="text-align:left">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec ultricies massa et erat luctus hendrerit. Curabitur non consequat enim. Vestibulum bibendum mattis dignissim. Proin id sapien quis libero interdum porttitor.</p>
                        </article>*@
                </div>
                <div class="clear"></div>
            </section>


            <section>
                <div class="sectionTitle">
                    <h1>Kĩ năng</h1>
                </div>

                <div class="sectionContent">
                    <article>
                        <p style="text-align:left">
                                Kĩ năng SV
                            </p>
                        
                    </article>
                </div>
                <div class="clear"></div>
            </section>


            <section>
                <div class="sectionTitle">
                    <h1>Chứng chỉ</h1>
                </div>

                <div class="sectionContent">
                    <article>
                        <h2>Chứng nhận 1</h2>
                        <p style="text-align:left" class="subDetails">2019</p>

                        <p style="text-align:left">Chứng nhận 1</p>
                    </article>

                    @*@if (Model.CV_Info.Certificate_Name_2 != null)
                    {
                        <article>
                            <h2>@Model.CV_Info.Certificate_Name_2</h2>
                            <p style="text-align:left" class="subDetails">@Model.CV_Info.Certificate_Date_Get_2</p>
                            @Html.Raw(Model.CV_Info.Certificate_Description_2)
                        </article>
                    }
                    @if (Model.CV_Info.Certificate_Name_3 != null)
                    {
                        <article>
                            <h2>@Model.CV_Info.Certificate_Name_3</h2>
                            <p style="text-align:left" class="subDetails">@Model.CV_Info.Certificate_Date_Get_3</p>
                            @Html.Raw(Model.CV_Info.Certificate_Description_3)
                        </article>
                    }*@
                </div>
                <div class="clear"></div>
            </section>

        </div>
    </div>

    <!------ALL JS HERE----->
    <script type="text/javascript">
        var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
        document.write(unescape("%3Cscript src='" + gaJsHost + "google-analytics.com/ga.js' type='text/javascript'%3E%3C/script%3E"));
    </script>
    <script type="text/javascript">
        var pageTracker = _gat._getTracker("UA-3753241-1");
        pageTracker._initData();
        pageTracker._trackPageview();
    </script>
</body>
</html>