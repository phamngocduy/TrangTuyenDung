            <script src="../../../assets/vendor_components/fullcalendar/fullcalendar.min.js"></script>*@
        <script src="~/Content/CSS_Company/Calendar/lib/moment.min.js"></script>
        <script src="~/Content/CSS_Company/Calendar/fullcalendar.min.js"></script>

        <!-- Fab Admin for calendar -->
        @*<script src="../../js/pages/calendar.js"></script>*@
        <script src="~/Scripts/Js_Company/calendar.js"></script>

