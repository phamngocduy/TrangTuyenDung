@model TrangTuyenDung.Models.Apply_Recruitments
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Vị trí ứng tuyển: </label>
        <label class="col-lg-8" style="font-weight:bold">@Model.Recruitment.title</label>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Công ty: </label>
        <label class="col-lg-8">@Model.Recruitment.Company_Info.Name_Company</label>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Ngày ứng tuyển: </label>        
        <label class="col-lg-8">@Model.Date_Apply</label>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Ngày phỏng vấn: </label>
        <label class="col-lg-8">@Model.Date_Interview</label>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Ngày hoàn thành: </label>
        <label class="col-lg-8">@Model.Date_Complete</label>
    </div>
</div>
<hr />
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Ngày bắt đầu đi làm: </label>
        <label class="col-lg-8">@Model.Date_Work</label>
    </div>
</div>
<div class="form-group">
    <div class="row">
        <label class="col-md-3 lblTitle">Địa điểm làm việc: </label>
        <label class="col-lg-8">@Model.Address_Work</label>
    </div>
</div>
