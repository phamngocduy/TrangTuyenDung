<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>EJOB - @ViewBag.Title</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Assets/img/favicon.ico">
    <link rel="apple-touch-icon" href="~/Assets/img/favicon.ico">

    <!--Boostrap -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <!--Font-awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css" integrity="sha256-+N4/V/SbAFiW1MPBCXnfnP9QSN3+Keu+NlB+0ev/YKQ=" crossorigin="anonymous" />
    <!-- Root CSS -->
    <link href="~/Content/index.css" rel="stylesheet" />
    <!-- Navbar CSS -->
    <link rel="stylesheet" href="~/Content/Home/index.css" />
    @RenderSection("Styles", required: false)
</head>
<body>
    <!--Header -->
    @Html.Action("LayoutHeader", "Home", new { area = "" })

    <div class="mainContainer">
        @RenderBody()
    </div>

    <!--Footer -->
    @Html.Partial("_LayoutFooter")

    <!-- Jquery -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <!-- Boostrap -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <!-- Navbar fixed scrolling -->
    <script src="~/Scripts/navigator.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
